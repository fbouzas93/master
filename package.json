    "axios": "1.9.0",
    "dotenv": "16.5.0",
    "node-pg-migrate": "7.9.1"
  },
  "devDependencies": {
    "jest": "29.7.0",
    "supertest": "7.1.1",
    "nodemon": "3.1.10",
    "prettier": "3.5.3"
  }
}